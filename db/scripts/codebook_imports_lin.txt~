mongoimport -d <db_name> -c <collection name> --type tsv --file "<path to file>/<file_name>" --headerline

CAMEO.Countries

mongoimport -d db_sense -c cameo_countries --type tsv --file "/home/ayushman/projects/sense/GDELT2.0/codebooks/CAMEO.country.txt" --headerline

